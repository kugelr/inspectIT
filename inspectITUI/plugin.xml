<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="info.novatec.inspectit.rcp.application"
            name="inspectIT">
         <property
               name="windowImages"
               value="info.novatec.inspectit.rcp/icons/selfmade/16x16.png,src/main/resources/icons/selfmade/32x32.png,src/main/resources/icons/selfmade/48x48.png,src/main/resources/icons/selfmade/64x64.png,src/main/resources/icons/selfmade/128x128.png">
         </property>
         <property
               name="aboutText"
               value="%aboutText">
         </property>
         <property
               name="aboutImage"
               value="about.png">
         </property>
         <property
               name="startupForegroundColor"
               value="FFFFFF">
         </property>
         <property
               name="startupMessageRect"
               value="5,10,560,20">
         </property>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         <property
               name="appName"
               value="inspectIT">
         </property>
      </product>
   </extension>

</plugin>
